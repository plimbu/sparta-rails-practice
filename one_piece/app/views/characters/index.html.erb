
<div class="">
  <%= image_tag("welcome_text.png") %>
</div>
<%= stylesheet_link_tag "character" %>

<%= link_to 'Crews', controller: 'crews' %>

<%= link_to 'New Character', new_character_path %>

<%= link_to 'New Crew', new_crew_path %>

<table>
  <tr>
    <th>Name</th>
    <th>Power</th>
    <th>Crew</th>
    <th colspan="5"></th>
    <th></th>
  </tr>

  <% @characters.each do |character| %>
    <tr>
      <td><%= character.name %></td>
      <td><%= character.power %></td>
      <td><%= character.crew.name %></td>
      <td><%= link_to 'Show', character_path(character) %></td>
      <td><%= link_to 'Edit', edit_character_path(character) %></td>
      <td><%= link_to 'Destroy', character_path(character),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>

<%= image_tag("character_index_image.png", class: "index_image") %>
