<div class="">
  <%= image_tag("welcome_text.png", class: "welcome_text") %>
</div>
<%= stylesheet_link_tag "crew" %>

<%= link_to image_tag('character_text.png', class: "crews_text_crew"), controller: 'characters' %>

<%= link_to image_tag('character_new_text.png', class: "crews_text_new_character"), new_character_path %>

<%= link_to image_tag('crew_new_text.png', class: "crews_text_new_crew"), new_crew_path %>



<table>
  <tr>
    <th>Name</th>
    <th>Location</th>
    <th colspan="5"></th>
    <th></th>
  </tr>

  <% @crews.each do |crew| %>
    <tr>
      <td><%= crew.name %></td>
      <td><%= crew.location %></td>
      <td><%= link_to 'Show', crew_path(crew) %></td>
      <td><%= link_to 'Edit', edit_crew_path(crew) %></td>
      <td><%= link_to 'Destroy', crew_path(crew),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>

<%= image_tag("crew_index_image.png", class: "index_image") %>
